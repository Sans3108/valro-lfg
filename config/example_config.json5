{
  // A hex number representing a color (Format: 0x??????)
  // Will be used as the color for all embeds
  embedColor: 0xFFFFFF,

  // Valorant Rank Discord Emotes used in the /lfg command
  ranks: { 
    unranked: ':beginner:',
    iron: ':third_place:',
    bronze: ':fallen_leaf:',
    silver: ':second_place:',
    gold: ':first_place:',
    platinum: ':star:',
    diamond: ':sparkles:',
    ascendant: ':fire:',
    immortal: ':boom:',
    radiant: ':alien:'
  },

  // Discord emotes used as arrows in the /lfg command
  arrows: {
    left: ':arrow_left:',
    right: ':arrow_right:'
  },

  // Symbols used in the /rate command
  rating: {
    empty: '☆',
    full: '★'
  },

  // Role IDs for staff roles (e.g. Admin, Moderator)
  // Remove those and add your own
  staffRoles: ['123456789069420', '123456789069420'],

  // The text that shows up at the bottom of the bot's embeds (footer)
  footer: 'ValRo LFG'
}
